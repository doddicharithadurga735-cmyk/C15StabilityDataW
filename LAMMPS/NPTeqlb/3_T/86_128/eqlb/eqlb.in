variable START_TEMP equal 50
variable TARGET_TEMP equal 300
variable NSTEPS equal 1000000
variable RANDS equal 372309
variable POT string ../../../W_bjorkas09.eam.fs

units metal
dimension 3
boundary p p p
atom_style atomic
read_data ../vis.dat

pair_style eam/fs
pair_coeff * * ${POT} W W 

neighbor 1.0 bin
neigh_modify once no every 1 delay 0 check yes

velocity all create ${START_TEMP} ${RANDS}
fix fixnpt all npt temp ${START_TEMP} ${TARGET_TEMP} 0.1 iso 0.0 0.0 0.1
timestep 1.0E-3

thermo		200
thermo_style custom step dt temp pe ke press vol
thermo_modify norm no
dump  dump all custom 500000 equilibrateG.dump id type x y z

run ${NSTEPS}

write_restart equilibrateG.restart
